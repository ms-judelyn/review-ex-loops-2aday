#include <iostream>

int main() {
    // Get user input
    std::cout << "Enter an integer: ";
    int user_input;
    std::cin >> user_input;

    // Validate input is between 0 and 5
    if (user_input >= 0 && user_input <= 5) {
        // Print numbers from 0 to user input
        for (int i = 0; i <= user_input; ++i) {
            std::cout << i << std::endl;
        }
    } else {
        std::cout << "Please enter an integer between 0 and 5." << std::endl;
    }

    return 0;
}
