// includes the input and the output
#include <iostream>
//using the standard to avoid writing std::
using namespace std;

// the execution of the program begins
int main() {
    // Input an integer from the user
    int n;
    
    // enter the integer
    cout << "Enter an integer: ";
    //read users age input from console
    cin >> n;

    // Initialize a variable i to 0
    int i = 0;

    // While i <= n and i <= 5, print i and increment i by 1
    while (i <= n && i <= 5) {
        cout << i << endl;
        i++;
    }
    return 0;
}
